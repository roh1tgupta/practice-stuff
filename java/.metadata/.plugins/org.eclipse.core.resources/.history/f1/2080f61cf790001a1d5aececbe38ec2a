package com.app;
import java.util.Date;

public class Book {
	String title;
	String author;
	double price;
	Date publishDate;
	String category;
	
	public Book(String title, String author, double price, Date publishDate, String category) {
		this.title = title;
		this.author =  author;
		this.price = price;
		this.publishDate = publishDate;
		this.category = category;
	}
	
	public String getCategory() {
		return this.category;
	}
	
	public String getTitle() {
		return this.title;
	}
	
	
}
